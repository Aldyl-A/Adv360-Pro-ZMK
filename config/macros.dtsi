complex_macro: complex_macro {
    label = "COMPLEX_MACRO";
    compatible = "zmk,behavior-macro";
    #binding-cells = <0>;
    wait-ms = <500>;
    tap-ms = <40>;
    bindings
        = <&macro_tap &kp N>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp P &kp P>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp E &kp P>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp R &kp E &kp N &kp E &kp W &kp E &kp D>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp N0>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp V &kp I &kp S &kp U &kp A &kp L>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp E &kp L &kp E &kp C &kp T &kp R &kp O>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp LC(DOWN)>
        , <&macro_wait_time 500>
        , <&macro_tap &kp N &kp FSLH &kp A>
        ;
};
