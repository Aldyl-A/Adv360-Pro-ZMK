macro_maximo1: macro_maximo1{
compatible = "zmk,behavior-macro";
label = "macro_maximo1";
#binding-cells = <0>;
bindings = <&kp NUMBER_0>, <&kp DOT>, <&kp NUMBER_7>, <&kp NUMBER_5>, <&kp TAB>, <&kp C_SLOW>, <&kp TAB>, <&kp C_SLOW>, <&kp TAB>, <&kp P>, <&kp P>;
};
